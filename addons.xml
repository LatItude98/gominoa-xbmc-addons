<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addons>
<addon id="repository.gomioa" name="gominoa's XBMC Addons" version="0.1.0" provider-name="gominoa">
	<extension point="xbmc.addon.repository" name="gominoa's XBMC Addon Repository">
		<info compressed="false">http://raw.github.com/gominoa/gominoa-xbmc-addons/master/addons.xml</info>
		<checksum>http://raw.github.com/gominoa/gominoa-xbmc-addons/master/addons.xml.md5</checksum>
		<datadir zip="true">http://raw.github.com/gominoa/gominoa-xbmc-addons/master/repo</datadir>
	</extension>
	<extension point="xbmc.addon.metadata">
		<summary lang="en">Install gominoa's Addons</summary>
		<description lang="en">Download and install addons by gominoa</description>
		<platform>all</platform>
	</extension>
</addon>

<addon id="plugin.audio.pandoraslim"
    name="Pandora Slim"
    version="0.4.2"
    provider-name="gominoa">
    <requires>
        <import addon="xbmc.python" version="2.1.0"/>
    </requires>
    <extension point="xbmc.python.pluginsource" library="default.py">
        <provides>audio</provides>
    </extension>
    <extension point="xbmc.addon.metadata">
	<summary lang="en">Pandora Internet Radio (Player Only)</summary>
	<description lang="en">Pandora Internet Radio (Player Only)</description>
	<disclaimer lang="en">Ain't my fault</disclaimer>
	<language></language>
	<platform>all</platform>
	<license>GNU GENERAL PUBLIC LICENSE Version 3, 29 June 2007</license>
	<forum>http://forum.xbmc.org/showthread.php?tid=205547</forum>
	<website></website>
	<email></email>
	<source>https://github.com/gominoa/gominoa-xbmc-addons</source>
    </extension>
</addon>

<addon id="plugin.audio.pandoki"
	name="Pandoki"
	version="0.7.4"
	provider-name="gominoa">
	<requires>
		<import addon="xbmc.python" version="2.1.0"/>
		<import addon="script.module.mutagen" version="1.22.0"/>
	</requires>
	<extension point="xbmc.python.pluginsource" library="default.py">
		<provides>audio</provides>
	</extension>
	<extension point="xbmc.addon.metadata">
		<summary lang="en">Pandora Internet Radio</summary>
		<description lang="en">Pandora Internet Radio</description>
		<disclaimer lang="en">Ain't my fault</disclaimer>
		<language></language>
		<platform>all</platform>
		<license>GNU GENERAL PUBLIC LICENSE Version 3, 29 June 2007</license>
		<forum>http://forum.xbmc.org/showthread.php?tid=205547</forum>
		<website></website>
		<email></email>
		<source>https://github.com/gominoa/gominoa-xbmc-addons</source>
	</extension>
</addon>
</addons>

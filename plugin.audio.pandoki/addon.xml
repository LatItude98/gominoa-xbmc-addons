<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addon id="plugin.audio.pandoki"
    name="Pandoki"
    version="0.10.4"
    provider-name="gominoa, rocky4546, truckershitch">
    <requires>
        <import addon="xbmc.python" version="3.0.0"/>
        <import addon="script.module.urllib3" version="1.25.8+matrix.1"/>
    </requires>
    <extension point="xbmc.python.pluginsource" library="default.py">
        <provides>audio</provides>
    </extension>
    <extension point="xbmc.addon.metadata">
        <summary lang="en_GB">Pandora Internet Radio</summary>
        <description lang="en_GB">Pandora Internet Radio</description>
        <disclaimer lang="en_GB">Ain't my fault</disclaimer>
        <language></language>
        <platform>all</platform>
        <license>GNU GENERAL PUBLIC LICENSE Version 3, 29 June 2007</license>
        <forum>https://forum.xbmc.org/showthread.php?tid=205547</forum>
        <website>http://www.pandora.com/</website>
        <email></email>
        <source>https://github.com/rocky4546/gominoa-xbmc-addons</source>
        <assets>
            <icon>icon.png</icon>
            <fanart>fanart.jpg</fanart>
        </assets>

    </extension>
</addon>
